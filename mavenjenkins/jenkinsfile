pipeline
{

agent any

stages {
  stage('clone') {
    steps {
      checkout([$class: 'GitSCM', branches: [[name: '*/master1']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/niteenahuja/DevOpsMavenJenkins.git']]])
    }
  }

  stage('build') {
    steps {
   sh 'mvn clean package'
    }
  }

}

}
